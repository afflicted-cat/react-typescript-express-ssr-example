{
  "name": "react-typescript-express-ssr-example",
  "version": "1.0.0",
  "description": "A starter for a project using TypeScript ExpressJs and ReactJs with server side rendering.",
  "scripts": {
    "test": "run-s test:*",
    "test:code": "jest",
    "test:types": "tsc --noEmit",
    "test:lint": "tslint --project .",
    "analyze": "run-p analyze:*",
    "analyze:server": "source-map-explorer dist/index.*",
    "analyze:bundle": "source-map-explorer dist/bundle.*",
    "analyze:vendors": "source-map-explorer dist/vendors.*",
    "prestart": "rimraf dist",
    "prebuild": "rimraf dist",
    "start": "cross-env NODE_ENV=development PORT=3001 nodemon",
    "server": "cross-env NODE_ENV=production PORT=8081 node dist/index.js",
    "build": "run-p build:*",
    "build:client": "cross-env NODE_ENV=production TARGET=client webpack",
    "build:server": "cross-env NODE_ENV=production TARGET=server webpack"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weyheyhey/react-typescript-express-ssr-example.git"
  },
  "keywords": [
    "react-hot-loader",
    "boilerplate",
    "css-modules",
    "typescript",
    "webpack",
    "express",
    "react",
    "redux",
    "ssr"
  ],
  "author": "weyheyhey",
  "license": "Unlicense",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "classnames": "^2.2.6",
    "core-js": "^3.1.3",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "history": "^4.9.0",
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.11.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "serialize-javascript": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/classnames": "^2.2.8",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "@types/ramda": "^0.26.9",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.1",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-mock-store": "^1.0.1",
    "@types/serialize-javascript": "^1.5.0",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-env": "^1.13.9",
    "@types/webpack-hot-middleware": "^2.16.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "circular-dependency-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cz-customizable": "^6.2.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.5",
    "glob": "^7.1.4",
    "husky": "^2.4.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-style-loader": "^5.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "^1.5.3",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^2.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.2.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js, json}": [
      "prettier --write",
      "git add"
    ]
  }
}
